<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Lof_SocialLogin::social_elements" title="Lof SocialLogin">
                    <resource id="Lof_SocialLogin::report_social" title="Report" sortOrder="10">
                        <resource id="Lof_SocialLogin::report_view" title="View Report" sortOrder="10" />
                    </resource>
                    <resource id="Lof_SocialLogin::sociallogin" title="Settings" sortOrder="10">
                        <resource id="Lof_AdvancedReports::sociallogin_view" title="Settings" sortOrder="10" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Lof_SocialLogin::sociallogin_config" title="Lof Social Login"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
